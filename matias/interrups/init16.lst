   728                                  
   729                                  ; Recorrer bus PCI. Bochs solo soporta bus cero ya que no tiene "PCI to PCI bridge".
   730                                  ; Cada bus tiene hasta 32 dispositivos.
   731                                  ; Si el dispositivo es compuesto puede tener hasta 8 funciones.
   732                                  ; Formato puerto 0x0CF8:
   733                                  ; Bit 31 = Siempre a "1"
   734                                  ; Bits 30-24 = Reservado (siempre a "0")
   735                                  ; Bits 23-16 = Número de bus (0 en nuestra aplicación)
   736                                  ; Bits 15-11 = Número de dispositivo (0-31)
   737                                  ; Bits 10-8 = Número de función (0-7)
   738                                  ; Bits 7-2 = Número de registro
   739                                  ; Bits 1-0: Siempre cero.
   740                                  
   741                                  recorrer_bus_pci:
   742 000010FF 66BD00400000            	mov 	ebp, 0x4000     	; Primera dirección de entrada/salida para PCI.
   743 00001105 66BF000000C0            	mov 	edi, 0xC0000000 	; Primera dirección a asignar a dispositivos PCI.
   744 0000110B 66BE00000080            	mov 	esi, 0x80000000 	; Bus 0, dispositivo 0, función 0.
   745                                  
   746                                  ciclo_recorrer_bus_pci:
   747 00001111 6681FE00000180          	cmp 	esi, 0x80010000 	; Verificar si se recorrió todo el bus 0.
   748 00001118 0F843001                	jz 		fin_recorrido_pci
   749                                  								; Leer registro 0: Bits 31-16: device ID,
   750                                  leer_registro_pci 0        		; Bits 15-0: vendor ID.
   751 0000111C 66B800000000        <1>  mov eax, %1
   752 00001122 6609F0              <1>  or eax, esi
   753 00001125 BAF80C              <1>  mov dx, 0x0CF8
   754 00001128 66EF                <1>  out dx, eax
   755 0000112A BAFC0C              <1>  mov dx, 0x0CFC
   756 0000112D 66ED                <1>  in eax, dx
   757 0000112F 83F8FF                  	cmp 	ax, 0xFFFF      	; Vendor ID 0xFFFF no existe.
   758 00001132 7509                    	jnz 	dispositivo_hallado
   759 00001134 6681C600080000          	add 	esi,0x0800     		; Ir al siguiente dispositivo
   760 0000113B EBD4                    	jmp 	ciclo_recorrer_bus_pci
   761                                  
   762                                  dispositivo_hallado:			; Leer registro 0x0C: Bit 23: multiple function bit
   763                                  	leer_registro_pci 0x0C
   764 0000113D 66B80C000000        <1>  mov eax, %1
   765 00001143 6609F0              <1>  or eax, esi
   766 00001146 BAF80C              <1>  mov dx, 0x0CF8
   767 00001149 66EF                <1>  out dx, eax
   768 0000114B BAFC0C              <1>  mov dx, 0x0CFC
   769 0000114E 66ED                <1>  in eax, dx
   770 00001150 B508                    	mov 	ch, 8       	    ; Cantidad de funciones a procesar.
   771 00001152 660FBAE017              	bt 		eax, 23
   772 00001157 7202                    	jc 		procesar_funcion
   773 00001159 B501                    	mov 	ch, 1
   774                                  	
   775                                  procesar_funcion:				; Leer registro 0: Bits 31-16: device ID,
   776                                  	leer_registro_pci 0      	; Bits 15-0: vendor ID.
   777 0000115B 66B800000000        <1>  mov eax, %1
   778 00001161 6609F0              <1>  or eax, esi
   779 00001164 BAF80C              <1>  mov dx, 0x0CF8
   780 00001167 66EF                <1>  out dx, eax
   781 00001169 BAFC0C              <1>  mov dx, 0x0CFC
   782 0000116C 66ED                <1>  in eax, dx
   783 0000116E 83F8FF                  	cmp  	ax, 0xFFFF    		; Vendor ID 0xFFFF no existe.
   784 00001171 7523                    	jnz 	funcion_hallada   
   785                                  
   786                                  ir_a_siguiente_funcion:
   787 00001173 6681C600010000          	add 	esi, 0x0100    		; Ir a la siguiente función
   788 0000117A FECD                    	dec 	ch
   789 0000117C 75DD                    	jnz 	procesar_funcion
   790 0000117E 6681EE00010000          	sub 	esi, 0x0100     	; Ir al siguiente dispositivo
   791 00001185 6681CE00070000          	or		esi, 0x0700
   792 0000118C 6681C600010000          	add 	esi, 0x0100
   793 00001193 E97BFF                  	jmp 	ciclo_recorrer_bus_pci
   794                                  
   795                                  funcion_hallada:				; Leer registro 0x0C: Bits 22-16: header type
   796                                  	leer_registro_pci 0x0C
   797 00001196 66B80C000000        <1>  mov eax, %1
   798 0000119C 6609F0              <1>  or eax, esi
   799 0000119F BAF80C              <1>  mov dx, 0x0CF8
   800 000011A2 66EF                <1>  out dx, eax
   801 000011A4 BAFC0C              <1>  mov dx, 0x0CFC
   802 000011A7 66ED                <1>  in eax, dx
