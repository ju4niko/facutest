   805                                  	leer_registro_pci 0x08		; Leer clase y subclase.
   806 000011B1 66B808000000        <1>  mov eax, %1
   807 000011B7 6609F0              <1>  or eax, esi
   808 000011BA BAF80C              <1>  mov dx, 0x0CF8
   809 000011BD 66EF                <1>  out dx, eax
   810 000011BF BAFC0C              <1>  mov dx, 0x0CFC
   811 000011C2 66ED                <1>  in eax, dx
   812 000011C4 B110                    	mov 	cl, 0x10         	; Apuntar al primer Base Address Register (BAR)
   813                                  
   814                                  ciclo_llenar_BAR:
   815 000011C6 660FB6C1                	movzx 	eax, cl
   816 000011CA 6609F0                  	or 		eax, esi 
   817 000011CD BAF80C                  	mov		dx, 0x0CF8
