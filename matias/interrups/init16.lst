    17                                  USE16
    18                                  
    19                                  %ifndef ROM_LOCATION							; Macro definida externamente
    20                                  	%error "ROM_LOCATION no definido"
    21                                  %endif	
    22                                  
    23                                  ;--------------------------------------------------------------------------------
    24                                  ; Macros
    25                                  ;--------------------------------------------------------------------------------
    26                                  %define 	CR0_PE	0x00000001 					; Protection Enable
    27                                  %define	 	CR0_PG	0x80000000 					; Paginacion
    28                                  
    29                                  %macro 		leer_registro_pci 1 				; 1 parámetro
    30                                  	mov 	eax, %1
    31                                  	or 		eax, esi
    32                                  	mov		dx, 0x0CF8
    33                                  	out 	dx, eax    							; Escribir palabra de configuración.
    34                                  	mov 	dx, 0x0CFC
    35                                  	in 		eax, dx   							; Verificar si existe el dispositivo.
    36                                  %endmacro
